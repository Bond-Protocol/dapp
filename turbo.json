{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "sync": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "test:open": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^dev", "codegen", "typechain"],
      "cache": false,
      "outputs": []
    },
    "preview": {
      "dependsOn": ["build"],
      "outputs": ["dist/**", "build/**"]
    },
    "build": {
      "dependsOn": ["^build", "codegen", "generate"],
      "outputs": ["dist/**", "build/**"]
    },
    "lp": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "typechain": {
      "inputs": ["src/core/abis/**/*.json"],
      "outputs": ["src/**/contracts", "src/"]
    },
    "codegen": {
      "inputs": ["package.json", "src/graphql/"],
      "outputs": ["src/generated/graphql.ts"]
    },
    "generate": {
      "inputs": ["src/protocols", "src/tokens"]
    },
    "storybook": {
      "dependsOn": [],
      "inputs": ["src"]
    },
    "build:storybook": {
      "dependsOn": []
    },
    "format": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  }
}
