{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["^dev", "codegen", "typechain"],
      "cache": false,
      "outputs": []
    },
    "preview": {
      "dependsOn": ["build"],
      "outputs": ["dist/**", "build/**"]
    },
    "build": {
      "dependsOn": ["^build", "codegen", "typechain"],
      "outputs": ["dist/**", "build/**"]
    },
    "typechain": {
      "outputs": ["src/types/contracts"]
    },
    "codegen": {
      "outputs": ["src/genearted/graphql.ts"]
    },
    "lint": {
      "outputs": []
    }
  }
}
