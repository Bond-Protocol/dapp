  "dependencies": {
    "viem": "2.15.1",
    "formatters": "*",
    "encoding": "^0.1.13"
  },
  "devDependencies": {
    "types": "*",
    "@wagmi/cli": "^1.5.2",
    "concurrently": "^7.6.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "eslint-config-prettier": "8.5.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "prettier": "2.7.1",
    "ts-node": "^10.2.1",
    "tsc-alias": "^1.5.0",
    "tsconfig": "*",
    "typedoc": "^0.22.5",
    "typescript": "^5.2.2"
  },
  "description": "contract-library is a library for building applications that can interact with the Bond Protocol contracts from within JavaScript runtimes.",
